   "source": [
    "# WPP to reverse a string.\n",
    "# Sample string : \"1234abcd\"\n",
    "# Expected output : \"dcba4321\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "424834d6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rev():\n",
    "    m = input()\n",
    "    n = list(m)\n",
    "    n.reverse()\n",
    "    print(\"\". join(n))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "98ba0348",
   "metadata": {},
   "outputs": [
